<h1>All tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag, tag_path(tag) %>
      <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Really delete the tag?"}%>
    </li>
  <% end %>
</ul>
<%= link_to "Create a new article", new_article_path, class: 'Create a New Article' %>
